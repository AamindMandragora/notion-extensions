{
  "functions": {
    "api/app.py": {
      "runtime": "@vercel/python"
    },
    "api/notion.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/update_habits",
      "schedule": "0 6 * * *"
    }
  ]
}